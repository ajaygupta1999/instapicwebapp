<div class="container1" id="main-content">
	   	<div class="row" >
          <% photos.forEach(function(photo){ %>
			<div class="col-xs-12 col-sm-6 col-lg-3">
			  <a href="/instapic/<%=photo._id%>">
				<div class="img-boxs">
				   <div class="imges">
						<img src="<%= photo.img%>">
					</div>
					
	   				<div class="content">
                      <div class="user">
                      	<div class="userimage">
                      		<img src="<%= photo.author.id.avatar %>">
                      	</div>
                      	<div class="username">
                      		<h5><%=photo.author.id.fullname.substring(0 , 20)%>.</h5>
                      	</div>
						  <div class="likes">
							  <button type="button" class="btn btn-warning">
								  <i class="fas fa-heart"></i> <%= photo.likes.length %>
							  </button>
						  </div>
                      </div>
	   				  <div class="discription">
	   					   <P><%= photo.description.substring(0 , 55)%>..</P>
	   				  </div>
	   				</div>
					
	   			</div>
				</a>
	   		</div>	
			<% })%>  		   	
</div>
